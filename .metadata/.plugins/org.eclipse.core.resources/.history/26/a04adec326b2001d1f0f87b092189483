package view;

public class Aluno {

	public void Notas() {
		this.n1 = 10;
		this.n2 = 8.9;
		this.n3 = 9.4;
		this.n4 = 5.7;
	}
	
	double n1;
	double n2;
	double n3;
	double n4;
	
	System.out.println("N1: " + n1);
	System.out.println("N2: " + n2);
	System.out.println("N3: " + n3);
	System.out.println("N4: " + n4);
}
